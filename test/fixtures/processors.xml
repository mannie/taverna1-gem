          <biomart:MartURLLocation database="vega_mart_56" default="0" displayName="VEGA 36  (SANGER UK)" host="www.biomart.org" includeDatasets="" martUser="" name="vega" path="/biomart/martservice" port="80" serverVirtualSchema="default" virtualSchema="default" visible="1" redirect="0" />
